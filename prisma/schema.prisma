generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "cockroachdb"
  url      = env("DATABASE_URL")
}

model nfts {
  address       String   @id @unique
  createdAt     DateTime @default(now())
  updatedAt     DateTime @updatedAt
  samId         String
  name          String
  firstCreator  String
  currentHolder String
  background    String
  body          String
  head          String
  rank          String
  faction       String
  boost         String
  collection    String
}
